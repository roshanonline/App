{
  "expo": {
    "name": "EchoMemory",
    "slug": "echo-memory-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png", // You need to create this icon file
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png", // You need to create this splash screen file
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png", // Create this file
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png" // Create this file
    }
  }
}